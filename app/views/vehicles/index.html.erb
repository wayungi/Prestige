<main class="wrapper">
  <p class="notice">
  <%#! <%= notice %> 
  HEADER MARKER TEXT
  </p>
  <section class="menu">
      <%= render "partials/nav" %>
  </section>

  <section class="content slider-container">
    <h1>Latest models</h1>
    <div class="controls" id="customize-controls" aria-label="Carousel Navigation" tabindex="0">
      <span class="prev" data-controls="prev" aria-controls="customize" tabindex="-1">
          <i class="fas fa-angle-left fa-5x"></i>
      </span>
      <span class="next" data-controls="next" aria-controls="customize" tabindex="-1">
          <i class="fas fa-angle-right fa-5x"></i>          
      </span>
      <article class="my-slider" id="carousel">
        <% @vehicles.each do |vehicle| %>
          <%= render partial: "vehicles/theme", locals: { vehicle: vehicle } %>
        <% end %>
      </article>
    </div>
  </section>
</div>
