<main class="wrapper">
<div class="notice">
    <%= notice %> 
    <h1>Theme vehicles</h1>
  </div>

  <section class="menu">
      <%= render "partials/nav" %>
  </section>

  <section class="content">
    <div class="swiper">
      <div class="swiper-wrapper">
        <% @vehicles.each do |vehicle| %>
            <%= render partial: "vehicles/theme", locals: { vehicle: vehicle } %>
        <% end %>
      </div>
      <div class="swiper-button-prev"></div> 
      <div class="swiper-button-next"></div>
    </div>
  </section>

</main>